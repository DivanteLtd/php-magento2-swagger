{
    "name": "divante/magento2-rest-api-client",
    "description": "Magento2 REST API Client",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api",
        "magento",
        "magento2"
    ],
    "homepage": "http://gitlab.divante.pl/ibood/magento2-rest-api-client",
    "license": "proprietary",
    "authors": [
        {
            "name": "Krzysztof Kardasz",            
            "email": "kkardasz@divante.pl",
            "role": "Developer"
        },
        {
            "name": "Swagger and contributors",
            "homepage": "https://github.com/swagger-api/swagger-codegen"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "Swagger\\Magento2Client\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Swagger\\Magento2Client\\" : "test/" }
    }
}
